<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Edit_informatio">


    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="551dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="1dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="#4CAF50"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            android:title="@string/new_user"
            android:titleTextColor="#FFFFFF"
            app:title="@string/request_toolbar" />

        <TextView
            android:id="@+id/first_namev_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@drawable/ic_add_black_24dp"
            android:textSize="18sp"
            />
        <EditText
            android:id="@+id/firt_nameV_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="18sp"/>


        <TextView
            android:id="@+id/last_namev_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@drawable/ic_add_black_24dp"
            android:textSize="18sp"
            />
        <EditText
            android:id="@+id/last_nameV_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="18sp"/>


        <TextView
            android:id="@+id/phonev_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@drawable/ic_add_black_24dp"
            android:textSize="18sp"
            />
        <EditText
            android:id="@+id/phoneV_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="18sp"/>


        <TextView
            android:id="@+id/emailv_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@drawable/ic_add_black_24dp"
            android:textSize="18sp"
            />
        <EditText
            android:id="@+id/emalV_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/passwordv_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@drawable/ic_add_black_24dp"
            android:textSize="18sp"
            />
        <EditText
            android:id="@+id/passwordV_edit"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="18sp"/>
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
